    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test-sites": "node scripts/test.js --projects test-sites",
    "cosmos:export": "cosmos-export"
  },
  "jest": {
    "projects": [
      "./jest.config.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs, ts, tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "./src/test/setupTest.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs, ts, tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs, ts, tsx}"
    ],
    "testURL": "https://www.config-gamer.fr",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs",
      "ts",
      "tsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-info": "^3.4.11",
    "@storybook/addon-links": "^3.4.11",
    "@storybook/addons": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "@types/jest": "^23.3.9",
    "@types/jest-environment-puppeteer": "^4.0.0",
    "@types/match-sorter": "^2.2.0",
    "@types/rc-slider": "^8.6.0",
    "@types/react": "^16.7.3",
    "@types/react-rnd": "^7.4.3",
    "@types/react-table": "^6.7.13",
    "@types/react-tooltip": "^3.3.6",
    "@types/uuid": "^3.4.4",
    "jest-environment-puppeteer": "^4.3.0",
    "jest-puppeteer": "^4.3.0",
    "puppeteer": "^1.18.1",
    "react-cosmos": "^4.6.4",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^3.5.0",
    "typescript": "^3.1.3"
  }
}

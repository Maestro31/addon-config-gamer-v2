    "match-sorter": "^2.3.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "rc-slider": "^8.6.3",
    "react": "^16.6.1",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.12",
    "react-loading": "^2.0.3",
    "react-numeric-input": "^2.2.3",
    "react-responsive-modal": "^3.4.0",
    "react-rnd": "^9.0.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.1.0",
    "react-spring": "^6.1.5",
    "react-table": "^6.8.6",
    "react-tooltip": "^3.9.0",
